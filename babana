function frequencyTable(banana) {
    // Input validation
    if (!Array.isArray(banana)) {
        throw new TypeError('Input must be an array');
    }
    
    // Optimized counting with nullish coalescing operator
    const freq = {};
    for (const item of banana) {
        freq[item] = (freq[item] ?? 0) + 1;
    }
    return freq;
}
